      "type": "task",
      "label": "Docker Deployment Config",
      "description": "Create containerized deployment setup",
      "data": {
        "task_type": "deploy_config",
        "platform": "Docker",
        "services": ["API", "Database", "Web"]
      }
    },
    {
      "id": "block-6",
      "type": "decision",
      "label": "API Ready?",
